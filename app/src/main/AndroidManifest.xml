<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.khoj">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.example.khoj.Objects.KhojApp"
        android:allowBackup="true"
        android:icon="@mipmap/lookout"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/lookout"
        android:supportsRtl="true"
        android:theme="@style/Theme.Khoj"
        tools:replace="android:theme,android:allowBackup,android:icon">
        <activity android:name="com.example.khoj.main_ui.HostelList" />
        <activity android:name="com.example.khoj.main_ui.LoginActivity" />
        <activity
            android:name="com.example.khoj.main_ui.SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Khoj.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.khoj.main_ui.MainActivity" />
    </application>

</manifest>